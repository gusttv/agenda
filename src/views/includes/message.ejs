<% if (errors.length > 0) { %>
<p class="message error"> <% errors.forEach(error => {%>
        <%= error %> <br>
    <% }); %>
</p>
<% }; %>

<% if (success.length > 0) { %>
    <p class="message success"> <% success.forEach(success => {%>
            <%= success %> <br>
        <% }); %>
    </p>
<% }; %>
    

